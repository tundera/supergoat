    "directory": "examples/nextjs"
  },
  "author": {
    "name": "Philip Johnston",
    "url": "https://github.com/tundera"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "ts-node db/seed.ts"
  },
  "scripts": {
    "dev": "NODE_ENV='development' ts-node scripts/dev",
    "debug": "NODE_ENV='development' ts-node scripts/debug",
    "dev:next": "next dev",
    "debug:next": "NODE_OPTIONS='--inspect' next dev",
    "clean": "rimraf .next tsconfig.tsbuildinfo",
    "build": "NODE_ENV='production' ts-node scripts/build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node src/services/graphql/schema",
    "generate:chakra": "chakra-cli tokens @supergoat/theme",
    "watch:codegen": "graphql-codegen --watch",
    "watch:nexus": "ts-node-dev -r tsconfig-paths/register --transpile-only --respawn src/services/graphql/schema",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --project-token=4186469d590d",
    "forms:deploy": "formspree deploy -k $FORMSPREE_DEPLOY_KEY",
    "test": "run-s 'test:*'",
    "test:unit": "echo \"No tests yet\""
  },
